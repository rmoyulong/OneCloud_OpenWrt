# 修复在某些docker环境下网络卡顿的问题, 如果非必要请勿删除!
cat > /etc/resolv.conf <<EOF
search lan
nameserver 127.0.0.1
nameserver 192.168.1.1
options ndots:0
EOF
sh /etc/1.sh
